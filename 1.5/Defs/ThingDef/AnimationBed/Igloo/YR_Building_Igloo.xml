									<heldPawnCondition>
										<hediffDefs>
											<li>YR_Ejaculation_Hediff</li>
										</hediffDefs>
										<dummyForJoyIsActive>true</dummyForJoyIsActive>
									</heldPawnCondition>
								</pawnCondition>
