									</joyPawnCondition>
								</pawnCondition>
								<bedAnimationDef>YR_GloryHole_Penis_Insect</bedAnimationDef>
							</li>
							<li>
								<pawnCondition>
									<heldPawnCondition>
										<dummyForJoyIsActive>true</dummyForJoyIsActive>
									</heldPawnCondition>
								</pawnCondition>
