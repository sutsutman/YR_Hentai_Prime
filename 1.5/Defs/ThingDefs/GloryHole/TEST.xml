<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--엉덩이-->
	<ThingDef ParentName="YR_AnimationBedBase">
		<defName>TEST</defName>
		<label>TEST</label>
		<description>TEST</description>
		<graphicData>
			<texPath>Yuran/Things/Building/Furniture/GloryHole/Bed/Texture</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
		</graphicData>
		<uiOrder>100</uiOrder>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>50</costStuffCount>
		<costList>
			<WoodLog>100</WoodLog>
		</costList>
		<comps>
			<li Class="YR_Hentai_Prime_AnimationBed.CompProperties_AnimationBed">
				<!-- 묶였을 때 부여되는 헤디프 -->
				<addedHediffDefs>
					<li>YR_MilkingMachine_DisableNeed</li>
				</addedHediffDefs>
			</li>
			<li Class="YR_Hentai_Prime_AnimationBed.CompProperties_ToggleHediff">
				<!-- 버튼으로 리스트에 있는 헤디프를 순서대로 변경 -->
				<hediffDefs>
					<li>YR_PowerMode1_Hediff</li>
					<li>YR_PowerMode2_Hediff</li>
				</hediffDefs>
			</li>
			<!-- 팡팡 놀이 -->
			<li Class="YR_Hentai_Prime_AnimationBed.CompProperties_SpawnDummyForJoy">
				<thingDef>YR_DummyForJoy</thingDef>
				<animationDef>YR_Global_Animation_PanPanPlay_North_Sound</animationDef>
				<makeHediff>
					<heldPawnHediffSetting>
						<finishHediffSettings>
							<li>
								<hediffDef>YR_Ejaculation_Hediff</hediffDef>
							</li>
						</finishHediffSettings>
					</heldPawnHediffSetting>
					<!-- <joyPawnHediffSetting> -->
					<!-- <startHediffSettings> -->
					<!-- <li> -->
					<!-- <hediffDef>YR_PowerMode1_Hediff</hediffDef> -->
					<!-- <conditionHediffDefs> -->
					<!-- <li> -->
					<!-- <hediffDef>YR_PowerMode1_Hediff</hediffDef> -->
					<!-- <condition/> -->
					<!-- </li> -->
					<!-- </conditionHediffDefs> -->
					<!-- <removeWhenFinish>true</removeWhenFinish> -->
					<!-- </li> -->
					<!-- </startHediffSettings> -->
					<!-- </joyPawnHediffSetting> -->
				</makeHediff>
				<makeSound>
					<!-- 묶인 애를 기준으로 나는 소리 -->
					<heldPawnSound>
						<startSoundSettings>
							<li>
								<!-- 삽입시 응앙! 하는 소리 -->
								<soundDefs>
									<li>OohSound_1</li>
									<li>OohSound_2</li>
									<li>OohSound_3</li>
									<li>OohSound_4</li>
									<li>OohSound_5</li>
									<li>OohSound_6</li>
									<li>OohSound_7</li>
									<li>OohSound_8</li>
								</soundDefs>
								<!-- <conditionSoundSettings> -->
								<!-- <li> -->
								<!-- <soundDef/> -->
								<!-- <condition/> -->
								<!-- </li> -->
								<!-- </conditionSoundSettings> -->
							</li>
						</startSoundSettings>
						<randomSoundSettings>
							<li>
								<!-- 신음 -->
								<soundDefs>
									<li>OohSound_1</li>
									<li>OohSound_2</li>
									<li>OohSound_3</li>
									<li>OohSound_4</li>
									<li>OohSound_5</li>
									<li>OohSound_6</li>
									<li>OohSound_7</li>
									<li>OohSound_8</li>
								</soundDefs>
								<!-- 이 경우 1틱당 0.05프로의 확률로 소리가 남 -->
								<probability>0.005</probability>
							</li>
						</randomSoundSettings>
						<finishSoundSettings/>
					</heldPawnSound>
					<!-- 하는 애를 기준으로 나는 소리 -->
					<joyPawnSound>
						<finishSoundSettings>
							<li>
								<!-- 사정 소리 -->
								<soundDefs>
									<li>EjaculationSound_1</li>
									<li>EjaculationSound_2</li>
									<li>EjaculationSound_3</li>
									<li>EjaculationSound_4</li>
									<li>EjaculationSound_5</li>
									<li>EjaculationSound_6</li>
									<li>EjaculationSound_7</li>
									<li>EjaculationSound_8</li>
									<li>EjaculationSound_9</li>
									<li>EjaculationSound_10</li>
									<li>EjaculationSound_11</li>
									<li>EjaculationSound_12</li>
									<li>EjaculationSound_13</li>
									<li>EjaculationSound_14</li>
									<li>EjaculationSound_15</li>
									<li>EjaculationSound_16</li>
									<li>EjaculationSound_17</li>
									<li>EjaculationSound_18</li>
									<li>EjaculationSound_19</li>
									<li>EjaculationSound_20</li>
									<li>EjaculationSound_21</li>
									<li>EjaculationSound_22</li>
									<li>EjaculationSound_23</li>
									<li>EjaculationSound_24</li>
									<li>EjaculationSound_25</li>
								</soundDefs>
							</li>
						</finishSoundSettings>
					</joyPawnSound>
				</makeSound>
			</li>
			<li Class="YR_Hentai_Prime_AnimationBed.CompProperties_AnimationSetting">
				<pawnAnimationSetting>
					<pawnAnimationDef>YR_Global_Animation_BodyChange_Invisible</pawnAnimationDef>
				</pawnAnimationSetting>
				<bedAnimationList>
					<!-- 폰의 바디, 헤드 -->
					<li>
						<!-- YR_Dummy_BedAnimation는 아무 애니메이션 없는거 -->
						<bedAnimationDef>YR_Dummy_BedAnimation</bedAnimationDef>
					</li>
					<!-- 침대의 애니메이션 -->
					<li>
						<bedAnimationDef>YR_Dummy_BedAnimation</bedAnimationDef>
						<conditionBedAnimationDefs>
							<li>
								<condition>
									<dummyForJoyIsActive>true</dummyForJoyIsActive>
								</condition>
								<bedAnimationDef>YR_Polar_Body</bedAnimationDef>
							</li>
						</conditionBedAnimationDefs>
					</li>
					<li>
						<bedAnimationDef>YR_Dummy_BedAnimation</bedAnimationDef>
						<conditionBedAnimationDefs>
							<li>
								<condition>
									<dummyForJoyIsActive>true</dummyForJoyIsActive>
								</condition>
								<bedAnimationDef>YR_Polar_Arm</bedAnimationDef>
							</li>
						</conditionBedAnimationDefs>
					</li>
				</bedAnimationList>
			</li>
		</comps>
	</ThingDef>
</Defs>
